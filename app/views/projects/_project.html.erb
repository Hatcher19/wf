<tr>
	<% if current_user == project.user %>
  <td><%= project.project_name %></td>
  <td><%= project.project_type %></td>
  <td><%= link_to '', project, class: "icon-eye-open" %></td>
  <td><%= link_to '', edit_project_path(project), class: "icon-edit" %></td>
  <td><%= link_to '', project, class: "icon-trash", method: :delete, data: { confirm: 'Are you sure?' } %></td>
  <% end %>
</tr>